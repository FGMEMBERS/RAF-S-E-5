<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <sim include="Nasal/views.xml">

    <author>Emmanuel BARANGER (3D), Pierre GEOFFROY (FDM), Laurent HAYVEL (Livery)</author>
    <aircraft-version>0.2</aircraft-version>
    <status>Alpha</status>

    <fuel-fraction>1.0</fuel-fraction>

    <startup>
      <splash-texture>Aircraft/RAF-S-E-5/se5-splash.png</splash-texture>
    </startup>

    <sound>
      <path archive="y">Aircraft/RAF-S-E-5/Sounds/se5-sound.xml</path>
    </sound>

    <model>
      <path archive="y">Aircraft/RAF-S-E-5/Models/se5.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
    </model>

    <multiplay>
      <chat_display>1</chat_display>
      <generic> 
        <int type="int">0</int>  <!-- 0 immat -->
        <int type="int">0</int>  <!-- 1 immat -->
        <int type="int">0</int>  <!-- 2 immat -->
        <int type="int">0</int>  <!-- 3 immat -->
        <int type="int">0</int>  <!-- 4 immat -->
        <int type="int">0</int>  <!-- 5 immat -->
        <int type="bool">0</int> <!-- 6 smoke -->
        <int type="int">0</int>  <!-- 7 color -->
        <float n="0" alias="/instrumentation/doors/crew/position-norm"/>
        <float n="1" alias="/instrumentation/doors/passenger/position-norm"/>
      </generic>
    </multiplay>

    <help>
      <title>RAF SE 5</title>
      <line>______________________________________________</line>
    </help>

    <weight n="0">
      <name>Pilot</name>
      <weight-lb>180</weight-lb>
      <max-lb>240</max-lb>
      <min-lb>140</min-lb>
    </weight>

    <weight n="1">
      <name>Pax</name>
      <weight-lb>0</weight-lb>
      <max-lb>240</max-lb>
      <min-lb>0</min-lb>
    </weight>

    <weight n="2">
      <name>Baggage</name>
      <weight-lb>0</weight-lb>
      <max-lb>20</max-lb>
      <min-lb>0</min-lb>
    </weight>

    <menubar include="Dialogs/se5-menu.xml"/>

  </sim>

  <controls>
    <engines>
      <engine>
        <mixture archive="y"> 0.65 </mixture>
      </engine>
    </engines>
  </controls> 

  <input>
    <keyboard include="Nasal/se5-keyboard.xml"/>
  </input>

  <nasal>
    <doors>
      <file>Aircraft/RAF-S-E-5/Nasal/doors.nas</file>
    </doors>
    <se5>
      <file>Aircraft/RAF-S-E-5/Nasal/se5.nas</file>
      <file>Aircraft/RAF-S-E-5/Nasal/liveries.nas</file>
    </se5>
  </nasal>

</PropertyList>

