<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>inttour</object-name>
    <object-name>intstructure</object-name>

    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>intvitres</object-name>
  </effect>
  
  <!-- Pilote -->
  <model>
    <path>Aircraft/RAF-S-E-5/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -0.269 </x-m>
      <y-m>  0.016 </y-m>
      <z-m> -0.106 </z-m>
    </offsets>
  </model>

  <!-- Tableau de bord -->
  <model>
    <path>Aircraft/RAF-S-E-5/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -0.412 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.558 </z-m>
    </offsets>
  </model>

</PropertyList>
